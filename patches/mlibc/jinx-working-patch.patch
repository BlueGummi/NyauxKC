+  // fuck off bash
+  fd_set res_read_set;
+  fd_set res_write_set;
+  fd_set res_except_set;
+  FD_ZERO(&res_read_set);
+  FD_ZERO(&res_write_set);
+  FD_ZERO(&res_except_set);
